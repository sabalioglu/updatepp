    "start:lan": "EXPO_NO_TELEMETRY=1 expo start --lan --clear",
    "start:localhost": "EXPO_NO_TELEMETRY=1 expo start --localhost --clear",
    "dev": "EXPO_NO_TELEMETRY=1 expo start --clear",
